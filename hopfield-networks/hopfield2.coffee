## Constants
# Length (in blocks) of a canvas edge
CANVAS_SIZE = 64
# Length (in pixels) of a single canvas cel
BLOCK_SIZE_PX = 4
MEMORY_BLOCK_SIZE_PX = 2
# Time (in milliseconds) to wait between memory cell updates
CELL_UPDATE_INTERVAL = 5

CORRUPT_PROBABILITY = 0.1

## Derived constants
NUM_BLOCKS = CANVAS_SIZE * CANVAS_SIZE

## Global state
# Are we currently recalling?
in_recall = false


# Learn a weight matrix from a set of memories (bit vectors)
#
# If each bit vector has `N` elements, the learned matrix is `N * N`.
learn_weights = (memories) ->
    N = memories[0].length
    W = []
    for i in [0...N]
        W[i] = []
        for j in [0...N]
            weight = 0

            if i != j
                for memory in memories
                    weight += (2 * memory[i] - 1) * (2 * memory[j] - 1)

            W[i].push weight

    return W


# Get excitation of given node in the provided state
get_input = (state, weights, i) ->
    weights_in = weights[i]
    sum = 0
    for weight, j in weights_in
        sum += weight * state[j]
    return sum


# Initiate recall starting from the given state and the given learn
# weights.
#
# If provided, `node_callback` will be called whenever the state is
# updated with two arguments:
#
# - `idx`, the index of the node within the state data being updated
# - `value`, the new value
#
# `node_callback` is only called when new values are found -- not when
# we find that the node is in a stable state.
recall = (state, weights, interval=0, node_callback=null, finish_callback=null) ->
    # List node indices
    node_idxs = [0...state.length]

    num_no_change = 0

    run_full_iter = (iter_callback) ->
        no_change = true

        # Visit the nodes in random order (randomized asynchronous
        # update)
        node_idxs = _.shuffle(node_idxs)

        # Update the node with ID stored at position `i` in `node_idxs`
        update_node = (node_id_idx) ->
            node_id = node_idxs[node_id_idx]
            node_state = state[node_id]
            node_in = get_input(state, weights, node_id)

            new_state = if node_in >= 0 then 1 else 0
            state_changed = new_state != state[node_id]
            state[node_id] = new_state

            if state_changed
                no_change = false

                if node_callback?
                    node_callback(node_id, new_state)

            if node_id_idx < node_idxs.length - 1
                # Only pause for `interval` if we are committing some
                # change
                if state_changed
                    setTimeout((-> update_node(node_id_idx + 1)), interval)
                else
                    update_node(node_id_idx + 1)
            else
                if no_change
                    num_no_change += 1
                iter_callback()


        setTimeout((-> update_node(0)), interval)

    iter_callback = ->
        if num_no_change < 1
            run_full_iter(iter_callback)
        else if finish_callback?
            finish_callback()

    run_full_iter(iter_callback)


svg = d3.select("#main_canvas").append("svg")

# Mouse events
dragging = false
svg.on "mousedown", ->
    dragging = true
svg.on "mouseup", ->
    dragging = false


# Generate a matrix describing the currently drawn canvas
get_canvas_matrix = (canvas) ->
    M = []
    canvas.each (el) ->
        val = if d3.select(this).attr("class").indexOf("on") != -1 then 1 else 0
        M.push(val)

    return M


# Save the given canvas to the Saved list
save_canvas = (canvas) ->
    M = get_canvas_matrix(canvas)
    save_canvas_matrix(M)

save_canvas_matrix = (M, canvas=main_canvas) ->
    # Add to saved list
    canvas_size = Math.sqrt(M.length)
    block_size_px = MEMORY_BLOCK_SIZE_PX
    canvas_size_px = block_size_px * canvas_size
    svg = d3.select("#saved").append("svg")
        .attr("width", canvas_size_px).attr("height", canvas_size_px)
    draw_canvas(svg, make_canvas_nodes(M), canvas_size, block_size_px, canvas)

    return M


# Clear canvas
reset_canvas = (canvas) ->
    canvas.each (el) ->
        d3.select(this).attr("class", "canvas_block")


# Generate canvas nodes from a matrix (provide this to drawing
# functions)
make_canvas_nodes = (M) ->
    return ({"index": i, "state": state} for state, i in M)


# Redraw the given nodes on the canvas
update_canvas = (canvas, canvas_nodes) ->
    canvas.each (el, i) ->
        klass = if canvas_nodes[el.index].state == 1
                "canvas_block on"
            else
                "canvas_block"

        d3.select(this).attr("class", klass)


# Draw a new canvas on the SVG container
draw_canvas = (svg, canvas_nodes, canvas_size, block_size_px, main_ref) ->
    this_canvas = svg.selectAll(".canvas_block").data(canvas_nodes)
      .enter().append("rect")
        .attr("class", (d) -> if d.state == 1 then "canvas_block on" else "canvas_block")
        .attr("x", (d) -> Math.floor(d.index / canvas_size) * block_size_px)
        .attr("y", (d) -> d.index % canvas_size * block_size_px)
        .attr("width", block_size_px)
        .attr("height", block_size_px)
        .style("stroke", if main_ref? then 'none' else 'black')

    if not main_ref?
        this_canvas.on("mouseover", ->
            if dragging
                d3.select(this).attr("class", "canvas_block on")
        )
    else
        this_canvas.on "click", ->
            update_canvas(main_ref, canvas_nodes)

    return this_canvas


canvas_control_buttons = d3.selectAll("#canvas_controls button")

# Indicate to user that recall has begun
enter_recall_mode = ->
    canvas_control_buttons.property("disabled", true)
    in_recall = true

exit_recall_mode = ->
    canvas_control_buttons.property("disabled", false)
    in_recall = false


# Simulate recall starting from the given canvas
recall_on_canvas = (canvas) ->
    # OK, start recall (visually)
    enter_recall_mode()

    M = get_canvas_matrix(canvas)

    # Build memory collections
    memories = []
    d3.selectAll("#saved svg").each ->
        memories.push get_canvas_matrix(d3.select(this).selectAll(".canvas_block"))

    # Learn weights for these memories
    W = learn_weights(memories)

    # Will be called whenever a node's value changes
    node_update_callback = (idx, val) ->
        rect = d3.select(canvas[0][idx])

        if val == 1
            rect.attr("class", "canvas_block on trans")
        else
            rect.attr("class", "canvas_block trans")

    # Called once recall process finishes
    final_callback = ->
        exit_recall_mode()

    # Find a minimum-energy assignment for Hopfield network.
    # Writes on `M` in place.
    recall M, W, CELL_UPDATE_INTERVAL, node_update_callback, final_callback
    update_canvas(canvas, make_canvas_nodes(M))


# Number of blocks in a row / column
canvas_nodes = ({"index": i, "state": 0} for i in [0...NUM_BLOCKS])

svg_size = CANVAS_SIZE * BLOCK_SIZE_PX
svg.attr("width", svg_size).attr("height", svg_size)

main_svg = svg
main_canvas = draw_canvas(main_svg, canvas_nodes, CANVAS_SIZE, BLOCK_SIZE_PX)

d3.select("#add-memory").on "click", ->
    save_canvas(main_canvas)
    reset_canvas(main_canvas)

d3.select("#recall-memory").on "click", ->
    recall_on_canvas(main_canvas)

d3.select("#corrupt").on "click", ->
    main_canvas.each ->
        if Math.random() < CORRUPT_PROBABILITY
            el = d3.select(this)
            klass = el.attr("class")
            if klass.indexOf("on") == -1
                el.attr("class", "canvas_block on trans_slow")
            else
                el.attr("class", "canvas_block trans_slow")


